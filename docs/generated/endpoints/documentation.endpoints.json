    "id": "GET /api/docs/iml",
    "method": "GET",
    "url": "/api/docs/iml",
    "summary": "Generate iml files",
    "description": "Generate iml files them to docs/generated/endpoints/ directory",
    "tags": [
      "Documentation"
    ],
    "deprecated": false,
    "contentTypes": [],
    "pathParams": [],
    "queryParams": [
      {
        "name": "tag",
        "schema": {
          "type": "string"
        },
        "description": "Filter by specific tag (e.g., \"template\", \"docs\", \"api\")"
      },
      {
        "name": "includeContent",
        "schema": {
          "type": "string",
          "enum": [
